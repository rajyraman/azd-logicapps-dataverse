{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "applicationId": {
      "value": "${DATAVERSE_APPLICATION_ID}"
    },
    "applicationSecret": {
      "value": "${DATAVERSE_APPLICATION_SECRET}"
    },
    "dataverseUrl": {
      "value": "${DATAVERSE_URL}"
    },
    "allowedLocations": {
      "value": "${ALLOWED_LOCATIONS}"
    }
  }
}